<?xml version="1.0" encoding="UTF-8" ?>
<sketchPlan>
    <SCHEME_VERSION>{{ SCHEME_VERSION }}</SCHEME_VERSION>
    <commonInfo>
        <id></id>
        <externalId>{{ plan.id }}</externalId>
        <year>{{ plan.year }}</year>
        <periodYearFrom>{{ plan.year }}</periodYearFrom>
        <periodYearTo>{{ plan.year + 2 }}</periodYearTo>
        <name>План закупок на {{ plan.year }} год</name>
        <owner>
            <fullName>{{ foiv.fullName }}</fullName>
        </owner>
        <createDate>{{ plan.year }}-00-00T00:00:00Z</createDate>
        <confirmDate>{{ "now"|date("Y-m-d") }}T{{ "now"|date("h:i:s") }}Z</confirmDate>
    </commonInfo>
    <customerInfo>
        <fullName>{{ foiv.fullName }}</fullName>
        <factAddress>{{ foiv.factAddress }}</factAddress>
        <INN>{{ foiv.inn }}</INN>
        <KPP>{{ foiv.kpp }}</KPP>
    </customerInfo>
    <attachments>
        {% for doc in plan.planDocs %}
        <attachment>
            <fileName>{{ doc.file }}</fileName>
            <url>{{ doc_url }}{{ doc.file }}</url>
        </attachment>
        {% endfor %}
    </attachments>
</sketchPlan>